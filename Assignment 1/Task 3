#include <stdio.h>

int countPairs(int A[], int lengthA, int lengthB, int B[]) {
    
    int i, j, counter;
    counter = 0;
    j = 0;
    for (i = 0; i < lengthA; ++i) {
        while (A[i] == B[j]){
            ++counter;
            ++j;
        }
        printf("(%d,%d) ", A[i], counter);
        counter = 0;
    }
    return 0;
}

int main(){
    
    int i = 0;
    int lengthA, lengthB;
    int A[1000], B[1000];
    
    printf("Values of A separated by spaces (non-number to stop): ");
    while (scanf("%d", &A[i])) {
        ++i;
    }
    scanf("%*s");
    lengthA = i;
 
    i = 0;
    printf("Values of B separated by spaces (non-number to stop): ");
    while (scanf("%d", &B[i])) {
        ++i;
    }
    scanf("%*s");
    lengthB = i;
    //printf("%d\n1", lengthB);
    
    printf("Pairs: ");
    countPairs(A, lengthA, lengthB, B);
    return 0;
}
